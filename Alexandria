
public class Alexandria extends WonderBoard
{

	public void setRes() {
		resource = "Cloth";
	}

	public void setLoc() {
		location = "The Lighthouse of Alexandria";
	}

	public void addWonders() {
		list.add("3");
		list.add("IDK");
		list.add("7");
	}

	public void build() {
		list.build();
	}
	
}
