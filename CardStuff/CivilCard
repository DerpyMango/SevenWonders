public class CivilCard extends Card
{
	private int pointVal;
	public CivilCard(String c,String n, String co, int a,int p)
	{
		super(c,n,co,a);
		pointVal = p;
	}
	public int getPoints()
	{
		return pointVal;
	}
	public void print()
	{
		super.print();
		System.out.println("Victory Points: "+pointVal);
	}
	public String getType()
	{
		return "civil";
	}
}
