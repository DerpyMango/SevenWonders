
public class SciCard extends Card
{
	private String greenStatus;
	public SciCard(Cost c,String n, String co, int a,String g)
	{
		super(c,n,co,a);
		greenStatus = g;
	}
	public String getStatus()
	{
		return greenStatus;
	}
	public void print()
	{
		super.print();
		System.out.println("Green Status: "+greenStatus);
	}
	public String getType()
	{
		return "science";
	}
}
