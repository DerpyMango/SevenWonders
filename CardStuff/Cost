public class Cost 
{
	private int[] resourceCost;
	private String prerequisite;
	public Cost(int[] rc, String p)
	{
		resourceCost = rc;
		prerequisite = p;
	}
	public int[] getRCost()
	{
		return resourceCost;
	}
	public String getPrereq()
	{
		return prerequisite;
	}
	public void print()
	{
		System.out.println("Material Cost: "+resourceCost.toString());
	}
}
