
public class Babylon extends WonderBoard
{

	public void setRes() {
		resource = "Clay";
	}

	public void setLoc() {
		location = "The Hanging Gardens of Babylon";
	}

	public void addWonders() {
		list.add("3");
		list.add("L O A O E");
		list.add("7");
	}

	public void build() {
		list.build();
	}
	
}
