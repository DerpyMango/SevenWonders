
public class Babylon extends WonderBoard
{

	public void setRes() {
		resource = "Clay";
	}

	public void setLoc() {
		location = "The Hanging Gardens of Babylon";
	}

	public void addWonders() {
		list.add("7");
		list.add("Lit/Math/Engi");
		list.add("3");
	}

	public String build() {
		String thing = list.build();
		if(thing.contains("Lit"))
		{
			ending +=  thing + "/";
			return "";
		}
		else
		{
			VictoPoints.add(thing);
			return "";
		}
	}
	
}
