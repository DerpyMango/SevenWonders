
public abstract class WonderBoard {
	
	protected String location;
	protected SomeList list;
	protected String resource;
	protected SomeList VictoPoints;
	protected String ending;
	
	public WonderBoard()
	{
		list = new SomeList();
		VictoPoints = new SomeList();
		ending = "";
	}
	
	public String checkIfBuildable(int c)
	{
		
	}
	
	public abstract void setRes();
	
	public abstract void setLoc();
	
	public String getLoc()
	{
		return location;
	}
	
	public abstract void addWonders();
	
	public abstract String build();
}
