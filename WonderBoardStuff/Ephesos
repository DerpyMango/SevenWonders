
public class Ephesos extends WonderBoard
{

	public void setRes() {
		resource = "Papyrus";
	}

	public void setLoc() {
		location = "Ã‰phesos";
	}

	public void addWonders() {
		list.add("7");
		list.add("9C");
		list.add("3");
	}

	public String build() {
		String thing = list.build();
		if(thing.contains("C"))
			return thing;
		else
		{
			VictoPoints.add(thing);
			return "";
		}
	}
	
}
