
public class Rhodes extends WonderBoard
{

	public void setRes() {
		resource = "Ore";
	}

	public void setLoc() {
		location = "The Colossus of Rhodes";
	}

	public void addWonders() {
		list.add("7");
		list.add("2 A");
		list.add("3");
	}

	public String build() {
		String thing = list.build();
		if(thing.contains("A"))
			return thing;
		else
		{
			VictoPoints.add(thing);
			return "";
		}
	}
	
}
