
public class Alexandria extends WonderBoard
{

	public void setRes() {
		resource = "Cloth";
	}

	public void setLoc() {
		location = "The Lighthouse of Alexandria";
	}

	public void addWonders() {
		list.add("7");
		list.add("c/o/w/m");
		list.add("3");
	}

	public String build() {
		String thing = list.build();
		if(thing.contains("c"))
		{
			ending += thing + "\\";
			return "";
		}
		else
		{
			VictoPoints.add(thing);
			return "";
		}
	}
	
}
