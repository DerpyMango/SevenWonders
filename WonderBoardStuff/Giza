
public class Giza extends WonderBoard
{

	public void setRes() {
		resource = "Metal";
	}

	public void setLoc() {
		location = "The Pyramids of Giza";
	}

	public void addWonders() {
		list.add("7");
		list.add("5");
		list.add("3");
	}

	public String build() {
		String thing = list.build();
		VictoPoints.add(thing);
		return "";
	}
	
}
